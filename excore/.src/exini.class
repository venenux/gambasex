Private Function appname_Read() As String

    Dim pos As Integer
    $appname = Application.Name
    pos = String.InStr($appname, ".")
    $appname = String.Mid($appname, pos + 1)
    pos = String.InStr($appname, ".")
    $appname = String.Mid($appname, pos + 1)
    pos = String.InStr($appname, ".")
    $appname = String.Mid($appname, pos + 1)
    Return $appname

End


Private Function appconfig_Read() As Settings

    Return $appconfig

End

Private Sub appconfig_Write(Value As Settings)

    $appconfig = Value

End
